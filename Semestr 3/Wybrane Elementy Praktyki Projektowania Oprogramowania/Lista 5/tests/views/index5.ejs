<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
    </head>
    <body>
        <div>
                <form action="/print" method="post">
                <div>
                    <div>
                        <label for="imie">Imię</label>
                        <input type="text" id="imie" name="imie" required/>
                    </div>
                    <div >
                        <label for="nazwisko">Nazwisko</label>
                        <input type="text" id="nazwisko" name="nazwisko" required/>
                    </div>
                    <div>
                        <label for="nazwazajec">Nazwa zajęć</label>
                        <input type="text" id="nazwazajec" name="nazwazajec" required/>
                    </div>
                    <div>
                        <% var foo = 'zadanie' %>
                        <% for ( var i=1; i<=10; i++) { %>
                            <div>
                                <label for=<%= foo + i %>> Zadanie <%=  i %></label>
                                <input type="number" id=<%= foo + i %> name=<%= foo + i %> />  
                            </div>
                            <% } %>                            
                    </div>
                    <div>
                        <input type="submit" name="akceptacjaformularza" value="Zaakceptuj formularz"/>
                        <!-- <button>Zapisz</button> -->
                    </div>
                </div>
                </form>
        </div>
    </body>
</html>